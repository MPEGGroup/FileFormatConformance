{
  "problemMatcher": [
    {
      "owner": "pytest",
      "pattern": [
        {
          "regexp": "^(FAILURE):\\s(.+)$",
          "message": 2
        }
      ]
    },
    {
      "owner": "loguru",
      "pattern": [
        {
          "regexp": "^(ERROR|WARNING)\\s*:\\s(.+)$",
          "severity": 1,
          "message": 2
        }
      ]
    }
  ]
}
